{
  "name": "vantage",
  "version": "0.1.0",
  "private": true,
  "description": "Vantage - Intelligent Lead Generation Platform",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
